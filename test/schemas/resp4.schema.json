 {
   "definitions": {},
   "$schema": "http://json-schema.org/draft-07/schema#",
   "$id": "http://example.com/root.json",
   "type": "object",
   "title": "The Root Schema",
   "required": [
     "_type",
     "queryContext",
     "webPages",
     "rankingResponse"
   ],
   "properties": {
     "_type": {
       "$id": "#/properties/_type",
       "type": "string",
       "title": "The _type Schema",
       "default": "",
       "examples": [
         "SearchResponse"
       ],
       "pattern": "^(.*)$"
     },
     "queryContext": {
       "$id": "#/properties/queryContext",
       "type": "object",
       "title": "The Querycontext Schema",
       "required": [
         "originalQuery",
         "alteredQuery",
         "alterationDisplayQuery",
         "alterationOverrideQuery",
         "adultIntent"
       ],
       "properties": {
         "originalQuery": {
           "$id": "#/properties/queryContext/properties/originalQuery",
           "type": "string",
           "title": "The Originalquery Schema",
           "default": "",
           "examples": [
             "Timmothy Cruise"
           ],
           "pattern": "^(.*)$"
         },
         "alteredQuery": {
           "$id": "#/properties/queryContext/properties/alteredQuery",
           "type": "string",
           "title": "The Alteredquery Schema",
           "default": "",
           "examples": [
             "timothy cruise"
           ],
           "pattern": "^(.*)$"
         },
         "alterationDisplayQuery": {
           "$id": "#/properties/queryContext/properties/alterationDisplayQuery",
           "type": "string",
           "title": "The Alterationdisplayquery Schema",
           "default": "",
           "examples": [
             "timothy cruise"
           ],
           "pattern": "^(.*)$"
         },
         "alterationOverrideQuery": {
           "$id": "#/properties/queryContext/properties/alterationOverrideQuery",
           "type": "string",
           "title": "The Alterationoverridequery Schema",
           "default": "",
           "examples": [
             "+Timmothy Cruise"
           ],
           "pattern": "^(.*)$"
         },
         "adultIntent": {
           "$id": "#/properties/queryContext/properties/adultIntent",
           "type": "boolean",
           "title": "The Adultintent Schema",
           "default": false,
           "examples": [
             false
           ]
         }
       }
     },
     "webPages": {
       "$id": "#/properties/webPages",
       "type": "object",
       "title": "The Webpages Schema",
       "required": [
         "webSearchUrl",
         "totalEstimatedMatches",
         "value"
       ],
       "properties": {
         "webSearchUrl": {
           "$id": "#/properties/webPages/properties/webSearchUrl",
           "type": "string",
           "title": "The Websearchurl Schema",
           "default": "",
           "examples": [
             "https://www.bing.com/search?q=Timmothy+Cruise"
           ],
           "pattern": "^(.*)$"
         },
         "totalEstimatedMatches": {
           "$id": "#/properties/webPages/properties/totalEstimatedMatches",
           "type": "integer",
           "title": "The Totalestimatedmatches Schema",
           "default": 0,
           "examples": [
             16600000
           ]
         },
         "value": {
           "$id": "#/properties/webPages/properties/value",
           "type": "array",
           "title": "The Value Schema",
           "items": {
             "$id": "#/properties/webPages/properties/value/items",
             "type": "object",
             "title": "The Items Schema",
             "required": [
               "id",
               "name",
               "url",
               "isFamilyFriendly",
               "displayUrl",
               "snippet",
               "language",
               "isNavigational"
             ],
             "properties": {
               "id": {
                 "$id": "#/properties/webPages/properties/value/items/properties/id",
                 "type": "string",
                 "title": "The Id Schema",
                 "default": "",
                 "examples": [
                   "https://api.cognitive.microsoft.com/api/v7/#WebPages.2"
                 ],
                 "pattern": "^(.*)$"
               },
               "name": {
                 "$id": "#/properties/webPages/properties/value/items/properties/name",
                 "type": "string",
                 "title": "The Name Schema",
                 "default": "",
                 "examples": [
                   "Timothy Cruise (J), 58 - Canton, MA | Background Report at ..."
                 ],
                 "pattern": "^(.*)$"
               },
               "url": {
                 "$id": "#/properties/webPages/properties/value/items/properties/url",
                 "type": "string",
                 "title": "The Url Schema",
                 "default": "",
                 "examples": [
                   "https://www.mylife.com/timothy-cruise/e151428700530"
                 ],
                 "pattern": "^(.*)$"
               },
               "isFamilyFriendly": {
                 "$id": "#/properties/webPages/properties/value/items/properties/isFamilyFriendly",
                 "type": "boolean",
                 "title": "The Isfamilyfriendly Schema",
                 "default": false,
                 "examples": [
                   true
                 ]
               },
               "displayUrl": {
                 "$id": "#/properties/webPages/properties/value/items/properties/displayUrl",
                 "type": "string",
                 "title": "The Displayurl Schema",
                 "default": "",
                 "examples": [
                   "https://www.mylife.com/timothy-cruise/e151428700530"
                 ],
                 "pattern": "^(.*)$"
               },
               "snippet": {
                 "$id": "#/properties/webPages/properties/value/items/properties/snippet",
                 "type": "string",
                 "title": "The Snippet Schema",
                 "default": "",
                 "examples": [
                   "Timothy Cruise is 58 years old and was born on 05/15/1960. Timothy's Reputation Score is 3.45. Timothy Cruise lives in Canton, MA; previous cities include Sharon MA, Buzzards Bay MA and Brockton MA. Sometimes Timothy goes by various nicknames including Timothy J Cruice, Timothy J Cruise, Timmothy Cruise, Tim Cruise and Timmothy J Cruise."
                 ],
                 "pattern": "^(.*)$"
               },
               "language": {
                 "$id": "#/properties/webPages/properties/value/items/properties/language",
                 "type": "string",
                 "title": "The Language Schema",
                 "default": "",
                 "examples": [
                   "en"
                 ],
                 "pattern": "^(.*)$"
               },
               "isNavigational": {
                 "$id": "#/properties/webPages/properties/value/items/properties/isNavigational",
                 "type": "boolean",
                 "title": "The Isnavigational Schema",
                 "default": false,
                 "examples": [
                   false
                 ]
               }
             }
           }
         }
       }
     },
     "rankingResponse": {
       "$id": "#/properties/rankingResponse",
       "type": "object",
       "title": "The Rankingresponse Schema",
       "required": [
         "mainline"
       ],
       "properties": {
         "mainline": {
           "$id": "#/properties/rankingResponse/properties/mainline",
           "type": "object",
           "title": "The Mainline Schema",
           "required": [
             "items"
           ],
           "properties": {
             "items": {
               "$id": "#/properties/rankingResponse/properties/mainline/properties/items",
               "type": "array",
               "title": "The Items Schema",
               "items": {
                 "$id": "#/properties/rankingResponse/properties/mainline/properties/items/items",
                 "type": "object",
                 "title": "The Items Schema",
                 "required": [
                   "answerType",
                   "resultIndex",
                   "value"
                 ],
                 "properties": {
                   "answerType": {
                     "$id": "#/properties/rankingResponse/properties/mainline/properties/items/items/properties/answerType",
                     "type": "string",
                     "title": "The Answertype Schema",
                     "default": "",
                     "examples": [
                       "WebPages"
                     ],
                     "pattern": "^(.*)$"
                   },
                   "resultIndex": {
                     "$id": "#/properties/rankingResponse/properties/mainline/properties/items/items/properties/resultIndex",
                     "type": "integer",
                     "title": "The Resultindex Schema",
                     "default": 0,
                     "examples": [
                       0
                     ]
                   },
                   "value": {
                     "$id": "#/properties/rankingResponse/properties/mainline/properties/items/items/properties/value",
                     "type": "object",
                     "title": "The Value Schema",
                     "required": [
                       "id"
                     ],
                     "properties": {
                       "id": {
                         "$id": "#/properties/rankingResponse/properties/mainline/properties/items/items/properties/value/properties/id",
                         "type": "string",
                         "title": "The Id Schema",
                         "default": "",
                         "examples": [
                           "https://api.cognitive.microsoft.com/api/v7/#WebPages.0"
                         ],
                         "pattern": "^(.*)$"
                       }
                     }
                   }
                 }
               }
             }
           }
         }
       }
     }
   }
 }


